
package packaged3;


public class object {

	int x = 4;

	public static void main(String[] args) {

		object myobj = new object();

		System.out.println(myobj.x);

		}

}

#Prime program:
package packaged3;


public class prime {

		  public static void main(String[] args) {


		    int num = 20;

		    boolean flag = false;

		    for (int i = 2; i <= num / 2; i++) {

		     

		      if (num % i == 0) {

		        flag = true;

		        break;

		      }

		    }


		    if (!flag)

		      System.out.println(num + " is a prime number.");

		    else

		      System.out.println(num + " is not a prime number.");

		  }
